<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="regContext" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterFormController">
   <children>
      <Label layoutX="246.0" layoutY="26.0" text="Register">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="txtEmail" layoutX="214.0" layoutY="95.0" promptText="Enter Email" />
      <TextField fx:id="txtName" layoutX="214.0" layoutY="142.0" promptText="Full name" />
      <TextField fx:id="txtPassword" layoutX="214.0" layoutY="185.0" promptText="Password" />
      <Button layoutX="268.0" layoutY="236.0" mnemonicParsing="false" onAction="#registerOnAction" text="Register" />
      <Label layoutX="179.0" layoutY="290.0" text="Already have an account?" />
      <Hyperlink layoutX="367.0" layoutY="286.0" onAction="#signInOnAction" text="SignIn" />
   </children>
</AnchorPane>
